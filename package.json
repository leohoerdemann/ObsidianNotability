{
	"name": "obsidian-nota-plugin",
	"version": "1.0.0",
	"description": "Notability-like note-taking plugin for Obsidian with handwriting, drawing, and annotation capabilities",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"nota",
		"drawing",
		"handwriting",
		"pdf",
		"notability"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/file-saver": "^2.0.7",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"file-saver": "^2.0.5",
		"jszip": "^3.10.1",
		"konva": "^9.3.18",
		"pdf-lib": "^1.17.1",
		"pptxgenjs": "^3.12.0",
		"tesseract.js": "^5.0.0"
	}
}
